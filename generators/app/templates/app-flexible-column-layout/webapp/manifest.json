{
  "_version": "1.9.0",
  "start_url": "index.html",
  "sap.app": {
    "id": "<%= projectNamespace %>",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "dataSources": {
      "RMTSAMPLEFLIGHT": {
        "uri": "/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_belize_hcw",
      "sap_belize_hcb",
      "sap_belize_plus",
      "sap_belize"
    ],
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    }
  },
  "sap.ui5": {
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "<%= projectNamespace %>.i18n.i18n"
        }
      },
      "odata": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "RMTSAMPLEFLIGHT",
        "settings": {
          "defaultBindingMode": "TwoWay"
        },
        "preload": true
      }
    },
    "rootView": {
      "viewName": "<%= projectNamespace %>.view.App",
      "id": "rootView",
      "type": "XML",
      "async": true,
      "height": "100%"
    },
    "routing": {
      "routes": [
        {
          "name": "master",
          "pattern": "",
          "target": "master",
          "layout": "OneColumn"
        },
        {
          "name": "masterDetail",
          "pattern": "items/{itemId}",
          "target": ["master", "detail"],
          "layout": "TwoColumnsMidExpanded"
        }
      ],
      "targets": {
        "master": {
          "viewId": "masterView",
          "viewName": "Master",
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewId": "detailView",
          "viewName": "Detail",
          "controlAggregation": "midColumnPages"
        }
      },
      "config": {
        "routerClass": "sap.f.routing.Router",
        "async": true,
        "viewType": "XML",
        "viewPath": "<%= projectNamespace %>.view",
        "controlId": "app"
      }
    },
    "dependencies": {
      "minUI5Version": "<%= projectMinimumUI5Version %>",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {},
        "sap.uxap": {},
        "sap.ui.unified": {},
        "sap.ui.layout": {}
      }
    },
    "resources": {
      "js": [],
      "css": []
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  }
}
