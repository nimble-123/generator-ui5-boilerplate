{
  "_version": "1.9.0",
  "start_url": "index.html",
  "sap.app": {
    "id": "<%= projectNamespace %>",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "dataSources": {
      "mainService": {
        "uri": "/northwind/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    },
    "sourceTemplate": {
      "id": "sap.ui.ui5-template-plugin.2masterdetail",
      "version": "1.52.7"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_belize_hcw",
      "sap_belize_hcb",
      "sap_belize_plus",
      "sap_belize"
    ],
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    }
  },
  "sap.ui5": {
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "<%= projectNamespace %>.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true
      }
    },
    "rootView": {
      "viewName": "<%= projectNamespace %>.view.App",
      "id": "app",
      "type": "XML",
      "async": true,
      "height": "100%"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "<%= projectNamespace %>.view",
        "controlId": "appView",
        "controlAggregation": "detailPages",
        "bypassed": {
          "target": ["master", "notFound"]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": ["object", "master"]
        },
        {
          "pattern": "Products/{objectId}",
          "name": "object",
          "target": ["master", "object"]
        }
      ],
      "targets": {
        "master": {
          "viewName": "Master",
          "viewLevel": 1,
          "viewId": "master",
          "controlAggregation": "masterPages"
        },
        "object": {
          "viewName": "Detail",
          "viewId": "detail",
          "viewLevel": 2
        },
        "detailObjectNotFound": {
          "viewName": "DetailObjectNotFound",
          "viewId": "detailObjectNotFound"
        },
        "detailNoObjectsAvailable": {
          "viewName": "DetailNoObjectsAvailable",
          "viewId": "detailNoObjectsAvailable"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "<%= projectMinimumUI5Version %>",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ushell": {},
        "sap.collaboration": {}
      }
    },
    "resources": {
      "js": [],
      "css": []
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  }
}
